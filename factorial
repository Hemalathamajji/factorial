#include<stdio.h>
#include<conio.h>
int fact(int);
int fact(int a)
{
    if(a==1)
       return 1;
    else
       return a*fact(a-1);
}
void main ()
{
    int a;
    printf("Enter any integer:");
    scanf("%d",&a);
    printf("The factorial of %d is:",fact(a));
}


outpu:
Enter any integer:6
The factorial of 720 is:
